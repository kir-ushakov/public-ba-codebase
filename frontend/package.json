    "@typescript-eslint/parser": "8.29",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "fake-indexeddb": "^6.2.2",
    "jest": "^29.7.0",
