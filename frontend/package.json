{
  "name": "brainas.com",
  "version": "0.0.6",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:e2e": "ng serve --configuration e2e",
    "build-prod": "ng build --configuration production --output-path dist/app",
    "build-dev": "ng build --output-path dist/app --poll 500 --watch",
    "test": "jest",
    "test:ci": "jest --ci --runInBand --coverage",
    "lint": "ng lint --fix",
    "lint:style": "npx stylelint 'src/**/*.scss' --fix",
    "format": "prettier --write \"src/**/*.{ts,html,scss}\"",
    "e2e": "npx playwright test",
    "e2e:ui": "npx playwright test --ui",
    "e2e:debug": "npx playwright test --debug"
  },
  "private": true,
  "type": "module",
  "dependencies": {
    "@angular/animations": "^19.1.7",
    "@angular/cdk": "^19.1.5",
    "@angular/common": "^19.1.7",
