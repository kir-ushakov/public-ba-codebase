    "@angular/core": "^19.1.7",
    "@angular/forms": "^19.1.7",
    "@angular/material": "^19.1.5",
    "@angular/platform-browser": "^19.1.7",
    "@angular/platform-browser-dynamic": "^19.1.7",
    "@angular/router": "^19.1.7",
    "@angular/service-worker": "^19.1.7",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/camera": "^6.0.0",
    "@capacitor/cli": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@ionic/pwa-elements": "^3.3.0",
    "@ngxs/storage-plugin": "^19.0.0",
    "@ngxs/store": "^19.0.0",
    "compressorjs": "^1.2.1",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "idb": "^8.0.3",
    "mime": "^4.0.3",
    "ng-mocks": "^14.13.2",
    "normalize.css": "^8.0.1",
    "rxjs": "^7.8.1",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.8",
    "@angular-eslint/builder": "^19.1.0",
